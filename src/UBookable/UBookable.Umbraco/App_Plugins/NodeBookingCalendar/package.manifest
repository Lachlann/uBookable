{
    //you can define multiple editors
    propertyEditors: [
        {
            /*this must be a unique alias*/
            alias: "NodeBookingCalendar",
            /*the name*/
            name: "Node Booking Calendar",
            /*the icon*/
            icon: "icon-umb-contour",
            /*the HTML file we will load for the editor*/
            editor: {
                view: "~/App_Plugins/NodeBookingCalendar/NodeBookingCalendar.html"
            }
        }
    ]
    ,
    //array of files we want to inject into the application on app_start
    javascript: [
		'~/Scripts/ubookable/ubookable.js',
		'~/bower_components/angular-simple-calendar/dist/angular-simple-calendar.js',
		'~/App_Plugins/NodeBookingCalendar/NodeBookingListViewEditorDialog.controller.js',
        '~/App_Plugins/NodeBookingCalendar/NodeBookingCalendar.controller.js'
    ],
	css: [
	 '~/App_Plugins/NodeBookingCalendar/lib/calendar.css',
	 '~/App_Plugins/NodeBookingCalendar/lib/general.css'
	]
}